{
    "version": 2,
    "builds": [
        {
            "src": "app/__init__.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "app/__init__.py"
        }
    ]
}
